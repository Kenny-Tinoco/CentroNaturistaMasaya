<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:System="clr-namespace:System;assembly=mscorlib"
    >
    
    <!--#region Tamaño fuente botones menú lateral-->
    <System:Double x:Key="fontSizeBML">25</System:Double>
    <!--#endregion-->
    <!--#region Tamaño fuente titulos de ventana-->
    <System:Double x:Key="fontSizeTituloVentana">25</System:Double>
    <!--#endregion-->
    <!--#region Tamaño fuente - Data Grid Header-->
    <System:Double x:Key="fontSizeDataGridHeader">15</System:Double>
    <!--#endregion-->
    <!--#region Tamaño height - Data Grid Header-->
    <System:Double x:Key="heightColumnHeader">36</System:Double>
    <!--#endregion-->
    <!--#region Tamaño fuente - Data Grid Header-->
    <System:Double x:Key="fontSizeDataGridRows">15.5</System:Double>
    <!--#endregion-->
</ResourceDictionary>