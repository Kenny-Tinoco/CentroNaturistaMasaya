<Application x:Class="CentroNaturistaMasaya.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CentroNaturistaMasaya"
             StartupUri="UI/VentanaPrincipal.xaml"> 
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource/Values/dimens.xaml"/>
                <ResourceDictionary Source="Resource/Values/strings.xaml"/>
                <ResourceDictionary Source="Resource/Values/colors.xaml"/>
                <ResourceDictionary Source="Resource/Values/Styles/ventanaPrincipal.xaml"/>
                <ResourceDictionary Source="Resource/Values/Styles/ventanaProducto.xaml"/>
                <ResourceDictionary Source="Resource/Values/Styles/ventanaProveedor.xaml"/>
                <ResourceDictionary Source="Resource/Drawable/Icono/IconosVP.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
